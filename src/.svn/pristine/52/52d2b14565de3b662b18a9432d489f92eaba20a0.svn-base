<!DOCTYPE html>
<html xmlns:th="http://www.thymeleaf.org">
<head>
    <meta charset="utf-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta content="width=device-width, initial-scale=1" name="viewport" />
    <link th:href="@{/css/admin/base/form.css}" type="text/css" rel="stylesheet" />
    <link th:href="@{/css/admin/base/table.css}" type="text/css" rel="stylesheet" />
</head>
<body>
<div class="lg-block">
    <button id="back" class="button">返回</button>
    <div class="lg-block-title1" th:text="南京江宁公共交通集团有限公司+${documentVo.zgs}+发文单"></div>
    <div class="lg-block-content">
        <div id="id" th:text="${documentVo.id}"  style="display:none;"></div>
        <ul>
            <li>
                <div class="form-title">公文标题:</div>
                <pre class="pre-form-content" th:text="${documentVo.bt}"></pre>
            </li>
            <li>
                <div class="form-title">主题词:</div>
                <pre class="pre-form-content" th:text="${documentVo.ztc}"></pre>
            </li>
            <li>
                <div class="form-title">主办人和拟公文人:</div>
                <pre class="pre-form-content" th:text="${documentVo.userName}"></pre>
            </li>
            <li>
                <div class="form-title">密级:</div>
                <div class="pform-radio"><input name="mj" type="radio" value="1" th:checked="${documentVo.mj=='1'}" th:disabled="true"/><span>绝密</span></div>
                <div class="pform-radio" style="margin-left: 35px"><input name="mj" type="radio" value="2" th:checked="${documentVo.mj=='2' }" th:disabled="true"/><span>机密</span></div>
                <div class="pform-radio" style="margin-left: 35px"><input name="mj" type="radio" value="3" th:checked="${documentVo.mj=='3' }" th:disabled="true"/><span>秘密</span></div>
            </li>
            <!--<li>-->
                <!--<div class="form-title">门户网站发布:</div>-->
                <!--<div class="pform-radio"><input name="sffb" type="radio" value="1" th:checked="${documentVo.sffb=='1' }" th:disabled="true"/><span>是</span></div>-->
                <!--<div class="pform-radio" style="margin-left: 35px"><input name="sffb" type="radio" value="2" th:checked="${documentVo.sffb=='2' }" th:disabled="true"/><span>否</span></div>-->
            <!--</li>-->
            <li>
                <th:block th:if="${documentVo.htbh != null}">
                    <div class="form-title" >编号:</div>
                    <pre class="pre-form-content" th:text="${documentVo.htbh}"></pre>
                </th:block>

            </li>
            <input id="attachList" type="hidden" th:value="${documentVo.attachmentList}" />
            <li><button id="fjck" class="button-active" style="margin-left: 0px">附件查看</button></li>
        </ul>

        <form id="d_gs_notify_form" enctype="multipart/form-data">
            <ul>
                <div class="lg-block-title2">主送:</div>
                <li>
                    <ul>
                        <li>
                            <div class="form-group">
                            <label th:text="公司高管" for="zsgsgg" class="col-sm-2 control-label"></label>
                                <div class="col-sm-10">
                            <select name="zsgsgg"  class="form-content" id="zsgsgg" th:multiple="multiple" style="width:300px">
                                <th:block th:if="${not #lists.isEmpty(notifyObjectVoList)}">
                                    <div th:each="object : ${notifyObjectVoList}">
                                        <th:block th:if="${object.tzdxfl == '公司高管'}">
                                            <option th:value="${object.userName}" th:text="${object.tzdx}"></option>
                                        </th:block>
                                    </div>
                                </th:block>
                            </select>
                                </div>
                            </div>
                        </li>
                        <li>
                            <div class="form-group">
                            <label th:text="公司部门" class="col-sm-2 control-label"></label>
                                <div class="col-sm-10">
                            <select name="zsgsbm"  class="form-content" id="zsgsbm" th:multiple="multiple" style="width:300px" >
                                <th:block th:if="${not #lists.isEmpty(notifyObjectVoList)}">
                                    <div th:each="object : ${notifyObjectVoList}">
                                        <th:block th:if="${object.tzdxfl == '公司部门'}">
                                            <option th:value="${object.userName}" th:text="${object.tzdx}"></option>
                                        </th:block>
                                    </div>
                                </th:block>
                            </select>
                                </div>
                            </div>
                        </li>
                        <li>
                            <div class="form-group">
                                <label th:text="公司办公室" class="col-sm-2 control-label"></label>
                                <div class="col-sm-10">
                                    <select name="zsqtgsbgs"  class="form-content" id="zsqtgsbgs" th:multiple="multiple" style="width:300px" >
                                        <th:block th:if="${not #lists.isEmpty(notifyObjectVoList)}">
                                            <div th:each="object : ${notifyObjectVoList}">
                                                <th:block th:if="${object.tzdxfl == '公司办公室'}">
                                                    <option th:value="${object.userName}" th:text="${object.tzdx}"></option>
                                                </th:block>
                                                <th:block th:if="${object.tzdxfl=='集团部门'}">
                                                    <option th:value="${object.userName}" th:text="${object.tzdx}"></option>
                                                </th:block>
                                            </div>
                                        </th:block>
                                    </select>
                                </div>
                            </div>
                        </li>
                        <li>
                            <div class="form-group">
                            <label th:text="外部单位" class="col-sm-2 control-label">外部单位</label>
                                <div class="col-sm-10">
                            <input name="zswbdw" class="form-content" id="zswbdw" style="width:300px" type="text"  th:placeholder="请输入" />
                                </div>
                            </div>
                        </li>
                    </ul>
                </li>
                <div class="lg-block-title2">抄报:</div>
                <li>
                    <ul>
                        <li>
                            <div class="form-group">
                            <label th:text="公司高管" class="col-sm-2 control-label"></label>
                                <div class="col-sm-10">
                            <select name="cbgsgg"  class="form-content" id="cbgsgg" th:multiple="multiple" th:size="2" style="width:300px">
                                <th:block th:if="${not #lists.isEmpty(notifyObjectVoList)}">
                                    <div th:each="object : ${notifyObjectVoList}">
                                        <th:block th:if="${object.tzdxfl == '公司高管'}">
                                            <option th:value="${object.userName}" th:text="${object.tzdx}"></option>
                                        </th:block>
                                    </div>
                                </th:block>
                            </select>
                                </div>
                            </div>
                        </li>
                        <li>
                            <div class="form-group">
                            <label th:text="公司部门" class="col-sm-2 control-label"></label>
                                <div class="col-sm-10">
                            <select name="cbgsbm"  class="form-content" id="cbgsbm" th:multiple="multiple" th:size="2" style="width:300px" >
                                <th:block th:if="${not #lists.isEmpty(notifyObjectVoList)}">
                                    <div th:each="object : ${notifyObjectVoList}">
                                        <th:block th:if="${object.tzdxfl == '公司部门'}">
                                            <option th:value="${object.userName}" th:text="${object.tzdx}"></option>
                                        </th:block>
                                    </div>
                                </th:block>
                            </select>
                                </div>
                            </div>
                        </li>
                        <li>
                            <div class="form-group">
                                <label th:text="公司办公室" class="col-sm-2 control-label"></label>
                                <div class="col-sm-10">
                                    <select name="cbqtgsbgs"  class="form-content" id="cbqtgsbgs" th:multiple="multiple" style="width:300px" >
                                        <th:block th:if="${not #lists.isEmpty(notifyObjectVoList)}">
                                            <div th:each="object : ${notifyObjectVoList}">
                                                <th:block th:if="${object.tzdxfl == '公司办公室'||object.tzdxfl=='集团部门'}">
                                                    <option th:value="${object.userName}" th:text="${object.tzdx}"></option>
                                                </th:block>
                                            </div>
                                        </th:block>
                                    </select>
                                </div>
                            </div>
                        </li>
                        <li>
                            <div class="form-group">
                            <label th:text="外部单位" class="col-sm-2 control-label">外部单位</label>
                                <div class="col-sm-10">
                            <input name="cbwbdw" class="form-content" id="cbwbdw" type="text" style="width:300px" th:placeholder="请输入" />
                                </div>
                            </div>
                        </li>
                    </ul>
                </li>
                <div class="lg-block-title2">抄送:</div>
                <li>
                    <ul>
                        <li>
                            <div class="form-group">
                            <label th:text="公司高管" class="col-sm-2 control-label"></label>
                                <div class="col-sm-10">
                            <select name="csgsgg"  class="form-content" id="csgsgg" th:multiple="multiple" th:size="2" style="width:300px">
                                <th:block th:if="${not #lists.isEmpty(notifyObjectVoList)}">
                                    <div th:each="object : ${notifyObjectVoList}">
                                        <th:block th:if="${object.tzdxfl == '公司高管'}">
                                            <option th:value="${object.userName}" th:text="${object.tzdx}"></option>
                                        </th:block>
                                    </div>
                                </th:block>
                            </select>
                                </div>
                            </div>
                        </li>
                        <li>
                            <div class="form-group">
                            <label th:text="公司部门" class="col-sm-2 control-label"></label>
                                <div class="col-sm-10">
                            <select name="csgsbm"  class="form-content" id="csgsbm" th:multiple="multiple" th:size="2" style="width:300px" >
                                <th:block th:if="${not #lists.isEmpty(notifyObjectVoList)}">
                                    <div th:each="object : ${notifyObjectVoList}">
                                        <th:block th:if="${object.tzdxfl == '公司部门'}">
                                            <option th:value="${object.userName}" th:text="${object.tzdx}"></option>
                                        </th:block>
                                    </div>
                                </th:block>
                            </select>
                                </div>
                            </div>
                        </li>
                        <li>
                            <div class="form-group">
                                <label th:text="公司办公室" class="col-sm-2 control-label"></label>
                                <div class="col-sm-10">
                                    <select name="csqtgsbgs"  class="form-content" id="csqtgsbgs" th:multiple="multiple" style="width:300px" >
                                        <th:block th:if="${not #lists.isEmpty(notifyObjectVoList)}">
                                            <div th:each="object : ${notifyObjectVoList}">
                                                <th:block th:if="${object.tzdxfl == '公司办公室'||object.tzdxfl=='集团部门'}">
                                                    <option th:value="${object.userName}" th:text="${object.tzdx}"></option>
                                                </th:block>
                                            </div>
                                        </th:block>
                                    </select>
                                </div>
                            </div>
                        </li>
                        <li>
                            <div class="form-group">
                            <label th:text="外部单位" class="col-sm-2 control-label">外部单位</label>
                                <div class="col-sm-10">
                            <input name="cswbdw" class="form-content" id="cswbdw" type="text" style="width:300px" th:placeholder="请输入"/>
                                </div>
                            </div>
                        </li>
                    </ul>
                </li>
            </ul>
        </form>
        <li class="right">
            <button id="tz" class="button hover">通知</button>
        </li>
    </div>
</div>
<script th:src="@{/js/admin/document/d_gs_notify.js}" type="text/javascript"></script>
</body>
</html>