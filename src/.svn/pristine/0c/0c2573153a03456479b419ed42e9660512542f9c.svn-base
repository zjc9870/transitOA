<!DOCTYPE html>
<html xmlns:th="http://www.thymeleaf.org">
<head>
    <meta charset="UTF-8" />
    <title>发文通知记录表</title>
    <link th:href="@{/css/admin/base/table.css}" rel="stylesheet"  type="text/css"/>
</head>
<body>
<div class="row">
    <div class="col-md-12">
        <div class="portlet-body">
            <div id="tab" class="table-tab">
                <button id="wd" class="button">通知对象未读</button>
                <button id="yd" class="button-active">通知对象已读</button>
            </div>

            <table id="d-notify-record-table" class="table table-striped table-bordered table-hover">
                <thead>
                <tr class="table-title">
                    <th>公文标题</th>
                    <th>状态</th>
                    <th>通知类型</th>
                    <th>通知对象</th>
                    <th id="sj">时间</th>
                </tr>
                </thead>
                <tbody id="d-notify-record-tbody" class="mt-td-checkbox">
                <th:block th:if='${not #lists.isEmpty(documentVoListBytzsj)}'>
                    <tr th:each="doc : ${documentVoListBytzsj}">
                        <th:block>
                            <input type="hidden" id="id" th:value="${doc.id}" />
                            <input type="hidden" id="fwtzid" th:value="${doc.fwtzid}" />
                            <input type="hidden" id="userrole" th:value="${doc.loginUserRole}" />
                            <td th:text='${doc.bt}'></td>
                            <td>未读</td>
                            <td th:text="${doc.tzlx}"></td>
                            <td th:text="${doc.tzdx}"></td>
                            <td th:text="${doc.tzsj} ? ${doc.tzsj} : '-' "></td>
                            <!--<th:block th:if="${not #lists.isEmpty(doc.tzsj)}">-->
                                <!--<td>-</td>-->
                            <!--</th:block>-->
                            <!--<th:block th:if="${doc.tzsj !=''}">-->
                                <!--<td th:text="${doc.tzsj}"></td>-->
                            <!--</th:block>-->
                        </th:block>
                    </tr>
                </th:block>
                </tbody>
            </table>
        </div>
    </div>
</div>
<script th:src="@{/js/admin/document/d_notify_record.js}" type="text/javascript"></script>
</body>
</html>